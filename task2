package game;
import java.util.*;
public class Num {
	public static void main(String[]args)
	{
		int count=0;
		for(int i=0;i<5;i++)
		{
			
		Random ran=new Random();
		int n=ran.nextInt(100);
		Scanner s=new Scanner(System.in);
		System.out.println("enter a number");
		int b=s.nextInt();
		System.out.println("the system number is "+n);
		if(b==n)
		{
			System.out.println("entered number matches the system number");
			count++;
		}
		else if(b>n)
		{
			System.out.println("entered number is grater than system number");
		}
		else
		{
			System.out.println("entered number is less than system number");
		}
		}
		System.out.println("your total score is"+count);
		
	}

}
